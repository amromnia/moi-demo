# Environment Variables Example
# Copy this file to .env to configure the application

# ====== FRONTEND (Vite) ======
# Traffic Sync Feature Flag
# Set to 'false' to completely disable traffic sync feature (UI and API calls)
# Default: enabled
VITE_ENABLE_TRAFFIC_SYNC=true

# ====== BACKEND (Server) ======
# Enable Traffic Sync on Server
# Set to 'false' to disable traffic sync endpoint
# Default: enabled
ENABLE_TRAFFIC_SYNC=true

# ====== KIOSK REGISTRATION SERVICE ======
# Base URL for the kiosk registration service
TRAFFIC_SYNC_BASE_URL=https://your-kiosk-service.com

# Username for kiosk service authentication
TRAFFIC_SYNC_USERNAME=your_username

# Password for kiosk service authentication
TRAFFIC_SYNC_PASSWORD=your_password

# Server Port (Production)
# PORT=3000

# Dev Server Port
# MMM_PORT=3001

